

<% content_for :user_head do %>
<div class="row">
	<div class="small-12 columns text-right">
		<div class="ldpanel">
			<% if @current_user %>
				Hello, <%= @current_user.user_profile.firstname rescue @current_user.email %> |
				<% if @current_user.brand %>
					<%= link_to 'Brand Profile', brand_url %> | 
				<% end %>
				<% if @current_user.distributor %>
					<%= link_to 'Distributor Profile', distributor_url %> | 
				<% end %>
				<%= link_to 'logout', logout_url %>
			<% else %>
			<%= link_to 'Login', login_url %> or <%= link_to 'Register', new_user_url %> 
			<% end %>
		</div>
	</div>
</div>
<% end %>


<div class="row">
	<div class="small-4 columns">
		<div class="ldpanel profile">
			marketing
		</div>
	</div>
	<div class="small-8 columns">
		<div class="ldpanel products">
			blurbs
		</div>
	</div>

</div>