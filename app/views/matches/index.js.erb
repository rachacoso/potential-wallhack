
<% if @current_user.brand %>

	$('#match-<%=@country%>').animate({"left" : "30px", "top" : "22%"}, 500);
	$('#match-<%=@country%> a').attr("href", "<%= all_matches_url %>");
	$('#match-<%=@country%> a').css("background", "rgba(0,0,0,0.6)");
	$('#match-<%=@country%> a').removeAttr("data-remote");

	$('#match-title').animate({"left" : "30px"}, 500);
	
	$('<%=@rest %>').css('visibility', 'hidden');
	$('<%=@rest %>').addClass("match-menu-others");

	var reset = $('#match-reset');
	setTimeout(function() {
	    reset.css('visibility', 'visible');
	}, 500);
	
	$('#match-list').html("<%= j (render 'distributor_match_list') %>").animate({"left" : "29%"}, 500);
<% end %>
	
