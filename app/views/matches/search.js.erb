<% if @current_user.brand %>
	$('#match-list').hide().html("<%= j (render 'brand_match_list') %>").prepend("You searched for '<%= @query %>'").fadeIn(500);
<% else %>
	$('#match-list').hide().html("<%= j (render 'distributor_match_list') %>").prepend("You searched for '<%= @query %>'").fadeIn(500);
<% end %>
$(document).foundation();
$(document).foundation('tooltip', 'reflow');