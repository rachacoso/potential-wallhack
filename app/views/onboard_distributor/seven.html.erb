<%= render partial: "shared/topbar" %>
<%= render layout: "shared/panel" do %>
	<div class="row">
		<div class="medium-12 columns">
			<h1>Trade Shows</h1>
			<hr>
		</div>
	</div>

	<%= render partial: "shared/editcollblock", locals: { 
		headertitle: "Shows Participated in",
		coll: @trade_shows,
		identifier: "name",
		no_item_message: "No Trade Shows",
		coll_new_item: @new_trade_show,
		coll_new_item_text: "Add a Trade Show",
		#iscurrent: "true"
	} %>

	<%= button_to "Continue", onboard_distributor_eight_url, method: :get, class: "tiny radius success button right" %>

<% end %>