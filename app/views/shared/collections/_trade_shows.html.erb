	<div class="medium-5 columns">
		<%= f.text_field :name, label: "Trade Show Name", placeholder: 'Trade Show Name' %>
	</div>
	<div class="medium-3 columns">
		<%= f.text_field :date, label: "Date", placeholder: 'Date' , class: "datepick" %>
	</div>
