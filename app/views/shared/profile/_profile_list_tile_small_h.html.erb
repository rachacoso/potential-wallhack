
<div class="profile-list-tile-small">

	<div class="row collapse">
		<div class="medium-2 columns">
			<% if @current_user.subscriber? && action_name != "public_profile"  %>
				<%= link_to view_match_url(match.id, action_name) do %>
					<%= image_tag match.logo.url(:medium), width: "100", alt: "Logo" %>
				<% end %>
			<% else %>
			<span data-tooltip aria-haspopup="true" class="has-tip" title="<%= match.class.to_s %> logo hidden. Subscribe to view full profile"><%= image_tag match.logo.url(:public), width: "40", height: "40", alt: "Logo" %></span>
			<% end %>
		</div>
		<div class="medium-10 columns">
			<div class="row collapse">
				<div class="small-4 columns">
					<% if @current_user.subscriber? && action_name != "public_profile" %>
						<span style="font-weight: 200;font-size:1.3em;"><%= link_to match.company_name, view_match_url(match.id, action_name) %></span>
					<% else %>
						<span data-tooltip aria-haspopup="true" class="has-tip" title="<%= match.class.to_s %> name hidden. Subscribe to view full profile">
						<%= ("&blk12;" * match.company_name.length).html_safe %>
						</span>
					<% end %><br>
					<% if @current_user.brand %>
						<%= render partial: "shared/profile/distributor_rating", locals: {d: match} %>
					<% end %>
				</div><!--/.small-4-->
				<div class="small-8 columns">

					
				</div><!--/.small-8-->
			</div><!--/.row-->	
			<div class="row collapse">
				<div class="small-4 columns">
					&nbsp;
				</div><!--/.small-4-->
				<div class="small-8 columns">
					<% if @current_user.subscriber? && action_name != "public_profile"  %>
						<%= render partial:"shared/profile/components/indicators_h", locals:{match: match} %>
					<% end %>
				</div><!--/.small-8-->
			</div><!--/.row-->				

		</div><!--/.medium-11-->

	</div>
</div>