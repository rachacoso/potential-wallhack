<div class="profile-list-tile-small-sq">
	<div class="profile-list-tile-small-sq-top">
		<div class="profile-list-tile-small-sq-top-countries">
			<% match.export_countries.each do |ec| %>
				<%= ec.country %><br>
			<% end %>
		</div>		
		<div class="profile-list-tile-small-sq-top-inner">
			<% if @current_user.subscriber? && action_name != "public_profile" %>
				<h3><%= link_to match.company_name, view_match_url(match.id, action_name) %></h3>
			<% else %>
				<span data-tooltip aria-haspopup="true" class="has-tip" title="<%= match.class.to_s %> name hidden. Subscribe to view full profile">
				<%= ("&blk12;" * match.company_name.length).html_safe %>
				</span>
			<% end %>			
		</div>
	</div>

	<div class="profile-list-tile-small-sq-middle">
		<% if @current_user.brand %>
			<%= render partial: "shared/profile/distributor_rating", locals: {d: match} %>
		<% end %>
		<% if @current_user.subscriber? && action_name != "public_profile"  %>
		<div class="profile-logo-area">
			<% if match.logo_file_name %>
				<%= link_to view_match_url(match.id, action_name) do %>
					<%= image_tag match.logo.url(:medium), width: "90", alt: "Logo" %>
				<% end %>
			<% end %>
		</div>
		<% else %>
		<span data-tooltip aria-haspopup="true" class="has-tip" title="<%= match.class.to_s %> logo hidden. Subscribe to view full profile"><%= image_tag match.logo.url(:public), width: "40", height: "40", alt: "Logo" %></span>
		<% end %>
	</div>

	<div class="profile-list-tile-small-sq-bottom">
		<%= render partial:"shared/profile/components/indicators", locals:{match: match} %>
	</div>

</div>



