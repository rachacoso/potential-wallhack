<%= render partial: "shared/topbar" %>

<% content_for :subnav do %>
<div class="row">
	<div class="small-12 columns">
		<dl class="sub-nav">
		  <dd class="active"><%= link_to '#' do %> All <strong>Users</strong> <% end %></dd>
		</dl>
	</div>
</div>
<% end %>


<div class="row">
	<div class="small-10 small-centered columns panel">
		<% if flash[:notice] %>

				<div data-alert class="alert-box success round">
			  	<%= flash[:notice] %>
				  <a href="#" class="close">&times;</a>
				</div>
		<% end %>

		<%= render partial: @users, spacer_template: "shared/indexspacer" %>

	</div>
</div>
<br>

